programa {
  funcao inicio() {
  real vpm, l1, l2, l3

  escreva("Vamos somar o volume de um prisma!")
  escreva("\nDigite o primiro lado do prisma: ")
  leia(l1)
  escreva("Digite o segundo lado do prisma: ")
  leia(l2)
  escreva("Digite o terceiro lado do prisma: ")
  leia(l3)
  vpm = l1 * l2 * l3
  escreva("\nO volume do prisma é : ",vpm) 
  }
}
