programa {
  funcao inicio() {
  real num1, num2, num3,formula

  escreva("Informe um número: ")
  leia(num1)
  escreva("Informe um segundo número: ")
  leia(num2)
  escreva("Informe um terceiro número: ")
  leia(num3)
  formula = (num1 + num3)*num2
  escreva("O resultado de ",num1, " + ",num3," x ",num2," é igual a: ",formula)
  }
}